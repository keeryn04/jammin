{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api-python/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api-python/app.py"
    },
    {
      "src": "(.*)",
      "dest": "frontend/$1"
    }
  ]
}