{
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "server/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/app.py"
      },
      {
        "src": "(.*)",
        "dest": "frontend/$1"
      }
    ]
  }  